root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 2

[*.go]
indent_style = tab
indent_size = 4

[*.csv]
insert_final_newline = false

[*.txt]
insert_final_newline = false

[Makefile]
indent_style = tab

[*.md]
trim_trailing_whitespace = false
